{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "distances-test",
            "type": "shell",
            "command": "python3 -m composed-robot.distance.run_generator"
        },
        {
            "label": "hub",
            "type": "shell",
            "command": "python3 -m composed-robot.hub"
        },
        {
            "label": "avoid",
            "type": "shell",
            "command": "python3 -m composed-robot.avoid.run"
        },
        {
            "label": "distances",
            "type": "shell",
            "command": "python3 -m composed-robot.distance.node"
        },
        {
            "label": "avoid-group",
            "dependsOn": [
                "hub","distances","avoid"
            ]
        },
        {
            "label": "collection",
            "type": "shell",
            "command": "python3 -m composed-robot.collection.run",
        },
        {
            "label": "motor",
            "type": "shell",
            "command": "python3 -m composed-robot.collection.robot_node"
        },
        {
            "label": "collection-group",
            "dependsOn": [
                "hub","collection","motor"
            ]
        },
    ]
}